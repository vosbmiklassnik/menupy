class Player:
    def __init__(self, name="Без названия", age=0):
        self.name = name
        self.age = age
        self.car = None
    def __str__(self):
        return f"Игрок: {self.name}, Возраст: {self.age}"
    def get_info(self):
        return f"Игрок: {self.name}, Возраст: {self.age}"
    def set_age(self, age):
        self.age = age
    def assign_car(self, car):
        self.car = car
    def get_car_info(self):
        if self.car:
            return self.car.get_info()
        else:
            return "У игроков нет машины"

class Car:
    def __init__(self, name = 'Lamba', speed = 230):
        self.name = name
        self.speed = speed
    def __str__(self):
        return f"Машина: {self.name}, Скорость: {self.speed}"
    def get_info(self):
        return f"Машина: {self.name}, Скорость: {self.speed}"
    def set_name(self, name):
        self.name = name
    def set_speed(self, speed):
        self.speed = speed
